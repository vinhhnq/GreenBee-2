{%- assign bg_image = sst.background_image -%}
<style>
  #shopify-section-{{ section.id }} .distance {
    padding-top: {{ sst.distance_top | append: 'px' }};
    padding-bottom: {{ sst.distance_bottom | append: 'px' }};
    padding-left: {{ sst.px | append: 'px' }};
    padding-right: {{ sst.px | append: 'px' }};
    {% if sst.background_color != blank or bg_image.src != blank %}
      background: {{ sst.background_color }}{% if bg_image != blank %} url("{{ bg_image | img_url: '1920x' }}") no-repeat center{% endif %};
      background-size: cover;
    {% endif %}
  }
  #shopify-section-{{ section.id }} .gallery-image__item {
    border-radius: {{ sst.bd_radius | append: 'px' }};
  }
  #shopify-section-{{ section.id }} .title {
    color: {{ sst.c_t }};
  }
  #shopify-section-{{ section.id }} .desc {
    color: {{ sst.c_d }};
  }
  #shopify-section-{{ section.id }} .btn {
    color: {{ sst.btn_label_c }};
    background-color: {{ sst.btn_label_b }};
  }
  #shopify-section-{{ section.id }} .btn:hover {
    color: {{ sst.btn_label_ch }};
    background-color: {{ sst.btn_label_bh }};
  }
  {% for block in section.blocks %}
    {% assign bst = block.settings %}
    {%- if bst.title_1 != blank -%}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} .image-1 .title  {
        font-size: {{ bst.fz_t1 | divided_by: 10.0 | append: 'rem' }};
      }
    {%- endif -%}
    {%- if bst.desc_1 != blank -%}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} .image-1 .desc {
        font-size: {{ bst.fz_d1 | divided_by: 10.0 | append: 'rem' }};
      }
    {%- endif -%}
