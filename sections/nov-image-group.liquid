{%- assign sst = section.settings -%}
<div class="distance{% if sst.class != blank %} {{ sst.class }}{% endif %}{% if sst.animate == true %} wow fadeInUp animated{% endif %}">
  {% if sst.container == 'container' %}
    <div class="container">
  {% elsif sst.container == 'container-fluid' %}
    <div class="container-fluid">
  {% else %}
    <div class="container-full">
  {% endif %} 
    {% if sst.heading != blank or sst.subheading != blank %}
      <div class="title_section {{ sst.title_alignment }}">
        {% if sst.heading != blank %}
          <span class="title" style="color: {{ sst.heading_c }};">{{ sst.heading }}</span>
        {% endif %}
        {% if sst.subheading != blank %}
          <span class="sub_title" style="color: {{ sst.subheading_c }};">{{ sst.subheading }}</span>
        {% endif %}
      </div>
    {% endif %}

    <div class="row spacing-{{ sst.spacing }}">
      {% for block in section.blocks %}
        {%- assign bst = block.settings -%}
        <div class="{{ bst.wg_lg }} {{ bst.wg_md }} {{ bst.wg_sm }} block-{{ forloop.index }}{% if bst.class_group != blank %} {{ bst.class_group }}{% endif %}">
          <div class="row spacing-{{ sst.spacing }} spacing-sm-10">
            {% if bst.image_1 != blank %}
              <div class="{{ bst.wi1 }} {{ bst.wi1_md }} {{ bst.wi1_sm }} {{ bst.wi1_xs }}{% if bst.class_1 != blank %} {{ bst.class_1 }}{% endif %}">
                <div class="gallery-image__item image-1{% if sst.style_img_1 == true %} nov-sh-image-1{% endif %}{% if sst.style_img_2 == true %} nov-sh-image-2{% endif %}">
                  {% if bst.link_1 != blank %}
                    <a class="w-100" href="{{ bst.link_1 }}">
                  {% endif %}
                  {% assign img_url = bst.image_1 %}
                  <div class="respone_image" style="padding-top:{{ 1 | divided_by: img_url.aspect_ratio | times: 100}}%;">
                    <img class="lazyload"
                      src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
                      data-src="{{ img_url | img_url: '180x' }}"
                      data-srcset="{{ img_url | img_url: '180x' }} 180w,{{ img_url | img_url: '380x' }} 380w,{{ img_url | img_url: '580x' }} 580w,{{ img_url | img_url: '768x' }} 768w,{{ img_url | img_url: '900x' }} 900w,{{ img_url | img_url: '1080x' }} 1080w,{{ img_url | img_url: '1170x' }} 1200w,{{ img_url | img_url: '1512x' }} 1512w,{{ img_url | img_url: '1728x' }} 1728w, {{ img_url | img_url: '2048x' }} 2048w" data-sizes="auto" alt="{{ img_url.alt }}">
                  </div>
                  {% if bst.link_1 != blank %}
                    </a>
                  {% endif %}
                  {% if bst.title_1 != blank or bst.desc_1 != blank or bst.btn_1 != blank %}
                    <div class="gallery-caption w-100 position-absolute bottom-center b-2 text-center font-700">
                      {% if bst.title_1 != blank %}
                        <{% if bst.link_1 != blank %}a href="{{ bst.link_1 }}"{% else %}div{% endif %} class="title">{{ bst.title_1 }}{% if bst.link_1 != blank %}</a>{% else %}</div>{% endif %}
                      {% endif %}
                      {% if bst.desc_1 != blank %}
                        <div class="desc mt-5">{{ bst.desc_1 }}</div>
                      {% endif %}
                      {% if bst.btn_1 != blank %}
                        <a class="btn{% if bst.title_1 != blank or bst.desc_1 != blank %} mt-10{% endif %}">{{ bst.btn_1 }}</a>
                      {% endif %}
                    </div>
                  {% endif %}
                </div>
              </div>
            {% endif %}
            {% if bst.image_2 != blank %}
              <div class="{{ bst.wi2 }} {{ bst.wi2_md }} {{ bst.wi2_sm }} {{ bst.wi2_xs }}{% if bst.class_2 != blank %} {{ bst.class_2 }}{% endif %}">
                <div class="gallery-image__item image-2{% if sst.style_img_1 == true %} nov-sh-image-1{% endif %}{% if sst.style_img_2 == true %} nov-sh-image-2{% endif %}">
                  {% if bst.link_2 != blank %}
                    <a class="w-100" href="{{ bst.link_2 }}">
                  {% endif %}
                  {% assign img_url = bst.image_2 %}
                  <div class="respone_image" style="padding-top:{{ 1 | divided_by: img_url.aspect_ratio | times: 100}}%;">
                    <img class="lazyload"
                      src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
                      data-src="{{ img_url | img_url: '180x' }}"
                      data-srcset="{{ img_url | img_url: '180x' }} 180w,{{ img_url | img_url: '380x' }} 380w,{{ img_url | img_url: '580x' }} 580w,{{ img_url | img_url: '768x' }} 768w,{{ img_url | img_url: '900x' }} 900w,{{ img_url | img_url: '1080x' }} 1080w,{{ img_url | img_url: '1170x' }} 1200,{{ img_url | img_url: '1512x' }} 1512w,{{ img_url | img_url: '1728x' }} 1728w, {{ img_url | img_url: '2048x' }} 2048w" data-sizes="auto" alt="{{ img_url.alt }}">
                  </div>
                  {% if bst.link_2 != blank %}
                    </a>
                  {% endif %}
                  {% if bst.title_2 != blank or bst.desc_2 != blank or bst.btn_2 != blank %}
                    <div class="gallery-caption w-100 position-absolute bottom-center b-2 text-center font-700">
                      {% if bst.title_2 != blank %}
                        <{% if bst.link_2 != blank %}a href="{{ bst.link_2 }}"{% else %}div{% endif %} class="title">{{ bst.title_2 }}{% if bst.link_2 != blank %}</a>{% else %}</div>{% endif %}
                      {% endif %}
                      {% if bst.desc_2 != blank %}
                        <div class="desc mt-5">{{ bst.desc_2 }}</div>
                      {% endif %}
                      {% if bst.btn_2 != blank %}
                        <a class="btn{% if bst.title_2 != blank or bst.desc_2 != blank %} mt-10{% endif %}">{{ bst.btn_2 }}</a>
                      {% endif %}
                    </div>
                  {% endif %}
                </div>
              </div>
            {% endif %}
            {% if bst.image_3 != blank %}
              <div class="{{ bst.wi3 }} {{ bst.wi3_md }} {{ bst.wi3_sm }} {{ bst.wi3_xs }}{% if bst.class_3 != blank %} {{ bst.class_3 }}{% endif %}">
                <div class="gallery-image__item image-3{% if sst.style_img_1 == true %} nov-sh-image-1{% endif %}{% if sst.style_img_2 == true %} nov-sh-image-2{% endif %}">
                  {% if bst.link_3 != blank %}
                    <a class="w-100" href="{{ bst.link_3 }}">
                  {% endif %}
                    {% assign img_url = bst.image_3 %}
                    <div class="respone_image" style="padding-top:{{ 1 | divided_by: img_url.aspect_ratio | times: 100}}%;">
                      <img class="lazyload"
                        src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
                        data-src="{{ img_url | img_url: '180x' }}"
                        data-srcset="{{ img_url | img_url: '180x' }} 180w,{{ img_url | img_url: '380x' }} 380w,{{ img_url | img_url: '580x' }} 580w,{{ img_url | img_url: '768x' }} 768w,{{ img_url | img_url: '900x' }} 900w,{{ img_url | img_url: '1080x' }} 1080w,{{ img_url | img_url: '1170x' }} 1200,{{ img_url | img_url: '1512x' }} 1512w,{{ img_url | img_url: '1728x' }} 1728w, {{ img_url | img_url: '2048x' }} 2048w" data-sizes="auto" alt="{{ img_url.alt }}">
                    </div>
                  {% if bst.link_3 != blank %}
                    </a>
                  {% endif %}
                  {% if bst.title_3 != blank or bst.desc_3 != blank or bst.btn_3 != blank %}
                    <div class="gallery-caption w-100 position-absolute bottom-center b-2 text-center font-700">
                      {% if bst.title_3 != blank %}
                        <{% if bst.link_3 != blank %}a href="{{ bst.link_3 }}"{% else %}div{% endif %} class="title">{{ bst.title_3 }}{% if bst.link_3 != blank %}</a>{% else %}</div>{% endif %}
                      {% endif %}
                      {% if bst.desc_3 != blank %}
                        <div class="desc mt-5">{{ bst.desc_3 }}</div>
                      {% endif %}
                      {% if bst.btn_3 != blank %}
                        <a class="btn{% if bst.title_3 != blank or bst.desc_3 != blank %} mt-10{% endif %}">{{ bst.btn_3 }}</a>
                      {% endif %}
                    </div>
                  {% endif %}
                </div>
              </div>
            {% endif %}
            {% if bst.image_4 != blank %}
              <div class="{{ bst.wi4 }} {{ bst.wi4_md }} {{ bst.wi4_sm }} {{ bst.wi4_xs }}{% if bst.class_4 != blank %} {{ bst.class_4 }}{% endif %}">
                <div class="gallery-image__item image-4{% if sst.style_img_1 == true %} nov-sh-image-1{% endif %}{% if sst.style_img_2 == true %} nov-sh-image-2{% endif %}">
                  {% if bst.link_4 != blank %}
                    <a class="w-100" href="{{ bst.link_4 }}">
                  {% endif %}
                    {% assign img_url = bst.image_4 %}
                    <div class="respone_image" style="padding-top:{{ 1 | divided_by: img_url.aspect_ratio | times: 100}}%;">
                      <img class="lazyload"
                        src="data:image/svg+xml,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1 1'%3E%3C/svg%3E"
                        data-src="{{ img_url | img_url: '180x' }}"
                        data-srcset="{{ img_url | img_url: '180x' }} 180w,{{ img_url | img_url: '380x' }} 380w,{{ img_url | img_url: '580x' }} 580w,{{ img_url | img_url: '768x' }} 768w,{{ img_url | img_url: '900x' }} 900w,{{ img_url | img_url: '1080x' }} 1080w,{{ img_url | img_url: '1170x' }} 1200,{{ img_url | img_url: '1512x' }} 1512w,{{ img_url | img_url: '1728x' }} 1728w, {{ img_url | img_url: '2048x' }} 2048w" data-sizes="auto" alt="{{ img_url.alt }}">
                    </div>
                  {% if bst.link_4 != blank %}
