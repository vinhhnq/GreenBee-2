<style>
  .site-header {
    background-color: rgba({{ sst.bg | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }}, {{ sst.bg_opacity }}%);
  }
  .header-top {
    background-color: rgba({{ sst.bg_t | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }}, {{ sst.bg_t_opacity }}%);
    color: {{ sst.c_t }};
  }
  .header-top .header-top-right .header-cu-lang .dropdown .current-currency, .header-top .header-top-right .header-cu-lang .dropdown button, .header-top .header-top-right .header-cu-lang .dropdown-toggle .current-currency, .header-top .header-top-right .header-cu-lang .dropdown-toggle button{
    color: {{ sst.c_ti }};
  }
  .myaccount, .wishlist, .site-header__cart{
    background-color: {{ sst.bg_t }};
    color: {{ sst.c_ti }};
  }
  .header-contact {
    background-color: {{ sst.bgc_contact }};
  }
  .header-contact i {
    color:  {{ sst.c_ti }};
  }
  
  .header-top .header-top-right .myaccount, .header-top .header-top-right .wishlist, .header-top .header-top-right .site-header__cart {
    background-color: {{ sst.bg_button }};
  }
  .header-top .header-top-right .wishlist:hover, .header-top .header-top-right .site-header__cart:hover {
    background-color: {{ sst.hover_bg_color }};
  }
  {% if sst.bd_b_sm %}
    .header-mobile {
      border-bottom: 1px solid {{ sst.bd_c_sm }};
    }
  {% endif %}
  #header-sticky {
    background-color: rgba({{ sst.bg_sticky | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }}, {{ sst.bg_sticky_opacity }}%);
  }
</style>
