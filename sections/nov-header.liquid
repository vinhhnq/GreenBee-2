{% assign sst = section.settings %}
<div data-section-id="{{ section.id }}" data-section-type="nov-slick">
  <header class="site-header{% if sst.stickymenu == true %} sticky-header{% endif %}">
    <div class="header-top pt-md-10 pb-md-10 d-none d-md-block">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-lg-3 col-md-3 d-flex justify-content-start align-items-center">
            <div class="site-nav--btn d-flex d-lg-none hover:c_df">
              <i class="zmdi zmdi-menu"></i>
            </div>
            <div class="contentsticky_logo">
              {% if sst.logo %}
                <a href="{{ routes.root_url }}" class="site-header__logo d-block" style="max-width: {{ sst.w_logo }}px">
                  {% assign imagewidth = sst.logo.width %}
                  {%- assign imagealt = sst.logo.alt | default: shop.name | escape -%}
                  {{ sst.logo | image_url: width: imagewidth | image_tag:
                  loading: 'lazy',
                  class: 'img-fluid',
                  alt: imagealt,
                  widths: '200 300 400'
                  }}
                </a>
              {% else %}
                <div class="h2 site-header__logo d-inline-block mb-0"><a class="site-header__logo-link" href="/" itemprop="url">{{ shop.name }}</a></div>
              {% endif %}
            </div>
          </div>
          <div class="col-lg-6 col-md-7 align-items-center content-header-center-left">
            <div class="contentsticky_menu d-none d-lg-block">
              <nav id="AccessibleNav">{% include 'site-nav' %}</nav>
            </div>
          </div>
          <div class="col-lg-3 col-md-2 d-sm-flex header-top-right align-items-center justify-content-end">
            <div class="header-cu-lang d-flex align-items-center">
              {%- if sst.enable_language and localization.available_languages.size > 1 -%}
                <div class="footer-language d-none d-lg-block">
                  {% include 'nov-block-language', id: 'header-language' %}
                </div>
              {%- endif -%}
              {% if settings.show_multiple_currencies == true and sst.enable_currency %}
                <div class="footer-currency d-lg-block d-none">
                  {% include 'nov-block-currency', type: 'dropdown' %}
                </div>
              {% endif %}
            </div>
            <div class="contentsticky_myaccount site-header_myaccount pointer text-center">
              {% if shop.customer_accounts_enabled %}
                {% if customer %}
                  <a href="{{ shop.secure_url }}/account" rel="nofollow" title="{{ 'layout.customer.account' | t }}">
                    <span class="myaccount header-icon">
                      <i class="{{ sst.account }} position-relative"></i>
                    </span>
                  </a>
                {% else %}
                  <div data-toggle="modal" data-target="#popup__login">
                    <div class="myaccount position-relative header-icon" title="{{ 'layout.customer.account' | t }}">
                      <i class="{{ sst.account }} position-relative"></i>
                    </div>
                  </div>
                {% endif %}
              {% endif %}
            </div>
            <div class="contentsticky_wishlist header--wishlist">
              <a href="{{ settings.link_page_wishlist }}" class="wishlist position-relative d-flex justify-content-center align-items-center">
                <span class="position-relative d-flex" rel="nofollow" title="{{ 'layout.customer.wishlists' | t }}">
                  <i class="{{ sst.wishlist }} position-relative"></i>
                </span>
              </a>
            </div>
            <div class="contentsticky_cart cart_canvas">
              <div id="cart_block">
                <div class="header-cart pointer">
                  <div class="site-header__cart d-flex justify-content-center align-items-center">
                    <span id="_desktop_cart_count" class="site-header__cart-count" title="{{ 'layout.cart.title_cart' | t }}">
                      <span id="CartCount">{{ cart.item_count }}</span>
                    </span>
                    <div class="site-header__cart-icon d-flex">
                      <i class="rbb-icon-shopping-{{ sst.cart_type }}-{{ sst.cart }} position-relative"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="contentsticky_cart title-cart d-lg-block d-md-none">
              <span>{{ sst.cart_description }}</span>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="header-center header-search__parent d-none d-md-block position-relative">
      <div class="container">
        <div class="row">
          <div class="col-lg-3 d-flex align-items-center content-header-center-left">
              <div class="header-vertical position-relative">
                <div class="btn-vertical text-uppercase d-flex align-items-center justify-content-center">
                  <i class="rbb-icon-menu-10"></i>
                  <span>{{ 'layout.header.categories' | t }}</span>
                </div>
                {% render 'nov-vertical_menu' %}
              </div>
          </div>
          <div class="col-lg-9 content-header-center d-flex">
            <div class="w-100 d-flex block-right">
              <div class="content-header-center-right">
                <div class="site-header__search d-none d-md-block">
                  {% include 'nov-formsearch-inline' %}
                </div>
              </div>
              <div class="header-contact w-100">
                <div class="item-contact align-items-center d-flex">
                  <div class="icon-phone d-flex align-items-center justify-content-center"><a class="d-flex" href="tel:{{ sst.phone }}"><i class="{{ sst.phone_icon }}"></i></a></div>
                  <div class="address">
                    <div class="how-order">{{ 'layout.header.how_to_order' | t }}</div>
                    <div class="mb-0 text-uppercase">{{ sst.phone }}</div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="header-mobile d-md-none{% if sst.stickymenu_sm == true %} sticky-header-mobile{% endif %}">
      <div class="header-mobile-content">
        <div class="row align-items-center h-100">
          <div class="col-4 d-flex align-items-center">
            <div id="show-megamenu" class="item-mobile-top">
              <div class="icon-directional position-absolute"></div>
            </div>
            <div class="mobile-btn_search item-mobile-top">
              {% include 'icon-search', color: sst.c_icon_m %}
              <i class="zmdi zmdi-close"></i>
            </div>
            <div id="mobile_search">
              {% include 'nov-formsearch-inline' %}
              {% include 'nov-search-autocomplete' %}
            </div>
          </div>
          <div class="mobile_logo col-4 text-center">
            {% if sst.logo_mobile %}
            <a href="/" class="site-header__logo-image img-fluid d-inline-block">
              {% assign imagewidth = sst.logo_mobile.width %}
              {%- assign imagealt = sst.logo_mobile.alt | default: shop.name | escape -%}
              {{ sst.logo_mobile | image_url: width: imagewidth | image_tag:
              loading: 'lazy',
              class: 'img-fluid',
              alt: imagealt,
              widths: '200 300 400'
              }}
            </a>
            {% else %}
            <a class="site-header__logo-link" href="/" itemprop="url">{{ shop.name }}</a>
            {% endif %}
          </div>
          <div class="mobile_cart col-4 d-flex justify-content-end">
            <div class="cart_canvas">
              <div class="header-cart d-flex justify-content-center">
                <div class="site-header__cart pointer d-flex justify-content-center align-items-center position-relative">
                  <span class="cart-count position-absolute" title="{{ 'layout.cart.title_cart' | t }}">
                    <span id="CartCount">{{ cart.item_count }}</span>
                  </span>
                  <div class="site-header__cart-icon">
                    <i class="rbb-icon-shopping-bag-6"></i>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </header>
  {% include 'nov-search-canvas' %}
  {% if sst.stickymenu == true %}
    <div id="header-sticky" class="d-none d-md-block">
      <div class="bl_content row align-items-center">
        <div class="col-md-3 d-flex align-items-center">
          <div class="site-nav--btn d-xl-none hover:c_df">
            <i class="zmdi zmdi-menu"></i>
          </div>
          <div class="contentstickynew_logo d-flex align-items-center"></div>
        </div>
        <div class="col-md-9 d-flex align-items-center justify-content-end justify-content-xl-between">
          <div class="contentstickynew_menu d-none d-xl-block"></div>
          <div class="header-group-item d-flex align-items-center justify-content-end">
            <div class="search__btn-overlay pointer d-flex" nov-btn-act data-toggle="search_canvas" overlay>
              <i class="rbb-icon-search-10 position-relative"></i>
            </div>
            <div class="contentstickynew_myaccount site-header_myaccount pointer"></div>
            <div class="contentstickynew_wishlist"></div>
            <div class="contentstickynew_cart"></div>
          </div>
        </div>
      </div>
    </div>
  {% endif %}
  <div class="d-none"><nav id="AccessibleNavMobile">{% include 'site-nav-mobile' %}</nav></div>
</div>
