<style>
  #shopify-section-{{ section.id }} .distance {
    padding-top: {{ sst.distance_top }}px;
    padding-bottom: {{ sst.distance_bottom }}px;
    {% if sst.background_color != blank %}
      background-color: {{ sst.background_color }};
    {% endif %}
  }
  {% if sst.enable_mh %}
  @media(min-width:  2000px) {
    #shopify-section-{{ section.id }} .respone_image {
      max-height: {{ sst.mh }}px;
      padding-top: 0 !important;
    }
    #shopify-section-{{ section.id }} .image-entity {
      max-height: {{ sst.mh }}px;
      position: static;
    }
  }
  {% endif %}
  #shopify-section-{{ section.id }} .timer:before {
    background-color: rgba({{ sst.bg_load | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }}, {{ sst.bg_load_opacity }}%);
  }
  #shopify-section-{{ section.id }} .main-slider {
    border-radius: {{ sst.border_radius | append: 'px'}} ;
  }
  #shopify-section-{{ section.id }} .timer:before, #shopify-section-{{ section.id }} .dot-style-2 .slick-active:before {
    animation-duration: {{ sst.autoplay_speed | divided_by: 1000 | append: 's' }};
  }
  #shopify-section-{{ section.id }} [data-slick-to]:before {
    background-color: rgba({{ sst.c_dot | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }}, {{ sst.c_dot_opacity }}%);
  }
  #shopify-section-{{ section.id }} [data-slick-to][current]:before, #shopify-section-{{ section.id }} [data-slick-to]:hover:before {
    background-color: {{ sst.c_dot_a }};
  }
  #shopify-section-{{ section.id }} [data-slick-to][current] .main-slider svg circle {
    stroke: rgba({{ sst.bd_dot_a | color_to_rgb | remove: 'rgb(' | remove: 'rgba(' | remove: ')' | remove: ', 0.0' }}, {{ sst.bd_dot_opacity }}%);
    transition: all {{ sst.autoplay_speed | divided_by: 1000 | append: 's' }} cubic-bezier(0.39, 0.58, 0.57, 1);
  }
  #shopify-section-{{ section.id }} .main-slider .slick-dots button {
    background-color: {{ sst.c_dot }};
  }
  #shopify-section-{{ section.id }} .main-slider .slick-dots button:before {
    background-color: {{ sst.c_dot_a }};
  }
  {% for block in section.blocks %}
    {% assign bst = block.settings %}
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} {
      {% if settings.enable_rtl %}
        {{ bst.position | replace: 'Left', 'Right' | replace: 'left', 'right' }};
      {% else %}
        {{ bst.position }};
      {% endif %}
    }
    #shopify-section-{{ section.id }} .caption-img-element-{{ forloop.index }} {
      {% if settings.enable_rtl %}{{ bst.position_img | replace: 'Left', 'Right' | replace: 'left', 'right' }}{% else %}{{ bst.position_img }}{% endif %};
    }
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-1 {
      font-size: {{ bst.fz_cap1_xxl }}px;
      letter-spacing: {{ bst.let1 }}px;
      margin-bottom: {{ bst.mb1 }}px;
      line-height: {{ bst.line_height1 }};
      -webkit-text-stroke: {{ bst.ts1_w }}px {{ bst.ts1_c }};
      font-weight: {{ bst.fw1 }};
      font-style: {{ bst.fs1 }};
      line-height: {{ bst.lh_cap1 }};
    }
    #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-2 {
      font-size: {{ bst.fz_cap2_xxl }}px;
      letter-spacing: {{ bst.let2 }}px;
      margin-bottom: {{ bst.mb2 }}px;
      line-height: {{ bst.line_height2 }};
      font-weight: {{ bst.fw2 }};
      font-style: {{ bst.fs2 }};
      line-height: {{ bst.lh_cap2 }};
