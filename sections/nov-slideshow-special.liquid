          },
          {
            "type": "text",
            "id": "line_height2",
            "label": "Line height",
            "default": "1.5"
          },
          {
            "type": "number",
            "id": "mb2",
            "label": "Margin bottom (px)",
            "default": 20
          },
          {
            "type": "number",
            "id": "mb2_lg",
            "label": "Margin bottom desktop 1199px ↓ (px)",
            "default": 20
          },
          {
            "type": "number",
            "id": "mb2_md",
            "label": "Margin bottom tablet 991px ↓ (px)",
            "default": 20
          },
          {
            "type": "number",
            "id": "mb2_xs",
            "label": "Margin bottom mobile 575px ↓ (px)",
            "default": 20
          },
          {
            "type": "select",
            "id": "cap_animate_2",
            "label": "Text animate",
            "default": "bounce",
            "options": [
