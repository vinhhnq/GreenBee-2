      }
    }
    @media(max-width: 767px) {
      #shopify-section-{{ section.id }} .caption-img-element-{{ forloop.index }} {
        {% if settings.enable_rtl %}{{ bst.position_sm_img | replace: 'Left', 'Right' | replace: 'left', 'right' }}{% else %}{{ bst.position_sm_img }}{% endif %};
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} {
        {% if settings.enable_rtl %}{{ bst.position_sm | replace: 'Left', 'Right' | replace: 'left', 'right' }}{% else %}{{ bst.position_sm }}{% endif %};
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-1 {
        font-size: {{ bst.fz_cap1_sm }}px;
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-2 {
        font-size: {{ bst.fz_cap2_sm }}px;
      }
    }
    @media(max-width: 575px) {
      #shopify-section-{{ section.id }} .slide-image.s-{{ forloop.index }} {
        min-height: {{ sst.h_xs | append: 'px' }};
      }
      #shopify-section-{{ section.id }} .slide-image.s-{{ forloop.index }} .respone_image {
        min-height: {{ sst.h_xs | append: 'px' }};
      }
      #shopify-section-{{ section.id }} .caption-img-element-{{ forloop.index }} {
        {% if settings.enable_rtl %}{{ bst.position_xs_img | replace: 'Left', 'Right' | replace: 'left', 'right' }}{% else %}{{ bst.position_xs_img }}{% endif %};
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} {
        {% if settings.enable_rtl %}{{ bst.position_xs | replace: 'Left', 'Right' | replace: 'left', 'right' }}{% else %}{{ bst.position_xs }}{% endif %};
      }
      #shopify-section-{{ section.id }} .content-{{ forloop.index }} .caption-1 {
        font-size: {{ bst.fz_cap1_xs }}px;
